{
  "name": "homepage",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "npm run lint:ts && npm run lint:style",
    "e2e": "ng e2e",
    "lint:ts": "ng lint --fix",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less --fix",
    "pretty-quick": "pretty-quick",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "test-coverage": "ng test --code-coverage --watch=false",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev:ssr": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng run homepage:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build && ng run homepage:server",
    "build:client-and-server-bundles": "ng build --prod && ng run homepage:server:production",
    "build:client-and-server-bundles-dev": "ng build && ng run homepage:server:production",
    "prerender": "ng run homepage:prerender",
    "build:stats": "ng build --extra-webpack-config webpack.extra.js --stats-json",
    "analyze": "webpack-bundle-analyzer dist/browser/stats.json",
    "gzipper": "gzipper --skip-compressed --remove-larger",
    "compress": "gzipper compress ./src ./dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/cdk": "^13.2.6",
    "@angular/cli": "^13.2.4",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "^13.1.3",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/platform-server": "^13.2.5",
    "@angular/router": "~13.1.0",
    "@angular/service-worker": "~13.1.0",
    "@gorniv/ngx-transfer-http": "^2.2.9",
    "@ng-web-apis/common": "^2.0.0",
    "@ng-web-apis/universal": "^2.0.0",
    "@ngrx/component": "^13.0.2",
    "@nguniversal/builders": "^13.0.2",
    "@nguniversal/express-engine": "^13.0.2",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "date-fns": "^2.28.0",
    "express": "^4.17.3",
    "ng-lazyload-image": "^9.1.2",
    "ng-zorro-antd": "^13.1.1",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "xmlhttprequest": "^1.8.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.3",
    "@angular/compiler-cli": "~13.1.0",
    "@types/express": "^4.17.13",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "tailwindcss": "^3.0.23",
    "typescript": "~4.5.2"
  }
}
